#ifndef LIBSECP256K1_CONFIG_H
#define LIBSECP256K1_CONFIG_H

#cmakedefine USE_ASM_X86_64 1
#cmakedefine USE_EXTERNAL_ASM 1

#cmakedefine USE_EXTERNAL_DEFAULT_CALLBACKS 1

#cmakedefine USE_ECMULT_STATIC_PRECOMPUTATION 1
#define ECMULT_WINDOW_SIZE ${SECP256K1_ECMULT_WINDOW_SIZE}
#define ECMULT_GEN_PREC_BITS ${SECP256K1_ECMULT_GEN_PRECISION}

#cmakedefine ENABLE_MODULE_ECDH 1
#cmakedefine ENABLE_MODULE_RECOVERY 1
#cmakedefine ENABLE_MODULE_SCHNORRSIG 1
#cmakedefine ENABLE_MODULE_EXTRAKEYS 1

#cmakedefine ENABLE_OPENSSL_TESTS 1

#cmakedefine COVERAGE 1

#cmakedefine HAVE_LIBCRYPTO 1
#cmakedefine HAVE_VALGRIND 1
#cmakedefine USE_FORCE_WIDEMUL_INT128 1
#cmakedefine USE_FORCE_WIDEMUL_INT64 1

#endif /* LIBSECP256K1_CONFIG_H */
